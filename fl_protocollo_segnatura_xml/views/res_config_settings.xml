<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="res_config_settings_view_form" model="ir.ui.view">
            <field name="name">res.config.settings.view.form</field>
            <field name="model">res.config.settings</field>
            <field name="priority" eval="20"/>
            <field name="inherit_id" ref="sd_protocollo_pec.res_config_settings_view_form"/>
            <field name="arch" type="xml">

                <div id="msg_module_fl_protocollo_segnatura_xml" position="replace"/>
                <xpath expr="//div[@name='module_fl_protocollo_segnatura_xml']" position="after">
                        <div class="content-group">
                            <br/>
                            <div class="row">
                                <field name="segnatura_xml_invia_pec" class="col-lg-1 ml16"/>
                                <label for="segnatura_xml_invia_pec" />
                            </div>
                            <br/>
                            <div class="row">
                                <field name="segnatura_xml_invia_email" class="col-lg-1 ml16"/>
                                <label for="segnatura_xml_invia_email" />
                            </div>
                            <br/>
                            <div class="row">
                                <field name="segnatura_xml_parse" class="col-lg-1 ml16"/>
                                <label for="segnatura_xml_parse" />
                            </div>
                            <br/>
                            <div class="row">
                                <field name="valida_segnatura_xml_ingresso" class="col-lg-1 ml16"/>
                                <label for="valida_segnatura_xml_ingresso" />
                            </div>
                            <br/>
                            <div class="row">
                                <field name="sender_segnatura_xml_parse" class="col-lg-1 ml16"/>
                                <label for="sender_segnatura_xml_parse" />
                            </div>
                            <br/>
                        </div>
                </xpath>
            </field>
        </record>

    </data>
</odoo>