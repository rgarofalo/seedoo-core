<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">

		<record id="res_config_settings_view_form" model="ir.ui.view">
			<field name="name">res.config.settings.view.form</field>
			<field name="model">res.config.settings</field>
			<field name="priority" eval="20"/>
			<field name="inherit_id" ref="base.res_config_settings_view_form"/>
			<field name="arch" type="xml">

				<xpath expr="//div[hasclass('settings')]" position="inside">
					<div class="app_settings_block" data-string="Strutture" data-key="fl_set" groups="base.group_system">
						<div name="fl_set_settings">

							<h2>Configurazione</h2>
							<div class="row mt16 o_settings_container" name="extensions">
								<div class="col-12 col-lg-6 o_setting_box">
									<div class="o_setting_left_pane">
										<field name="module_fl_set_pa"/>
									</div>
									<div class="o_setting_right_pane">
										<label for="module_fl_set_pa"/>
										<div class="text-muted">
											Permette la creazione di struture come le Aree Organizzative Omogenne e le Unit√† Organizzative
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</xpath>

			</field>
		</record>

	</data>
</odoo>