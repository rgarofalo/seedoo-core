<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <data noupdate="0">

        <record id="action_sd_protocollo_protocollo_dashboard" model="ir.actions.client">
            <field name="name">Dashboard</field>
            <field name="tag">sd.protocollo.dashboard</field>
            <field name="res_model">sd.protocollo.protocollo</field>
            <field name="params" eval="{}"/>
        </record>

        <record id="action_sd_protocollo_protocollo_list" model="ir.actions.act_window">
            <field name="name">Protocolli</field>
            <field name="res_model">sd.protocollo.protocollo</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <field name="limit">20</field>
        </record>

        <record id="action_sd_protocollo_protocollo_list_bozza" model="ir.actions.act_window">
            <field name="name">Bozze</field>
            <field name="res_model">sd.protocollo.protocollo</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[('state', '=', 'bozza')]</field>
            <field name="limit">20</field>
        </record>

        <record id="action_sd_protocollo_registro_giornaliero_list" model="ir.actions.act_window">
            <field name="name">Registro Giornaliero</field>
            <field name="res_model">sd.protocollo.registro.giornaliero</field>
            <field name="view_mode">tree,form</field>
            <field name="limit">20</field>
        </record>

        <record id="action_sd_protocollo_registro_emergenza_list" model="ir.actions.act_window">
            <field name="name">Registro Emergenza</field>
            <field name="res_model">sd.protocollo.registro.emergenza</field>
            <field name="view_mode">tree,form</field>
            <field name="limit">20</field>
        </record>

        <record id="action_sd_protocollo_invio_list" model="ir.actions.act_window">
            <field name="name">Invio</field>
            <field name="res_model">sd.protocollo.invio</field>
            <field name="view_mode">tree,form</field>
            <field name="limit">20</field>
        </record>

        <record id="action_sd_protocollo_mezzo_trasmissione_list" model="ir.actions.act_window">
            <field name="name">Mezzo di Trasmissione</field>
            <field name="res_model">sd.protocollo.mezzo.trasmissione</field>
            <field name="view_mode">tree,form</field>
            <field name="limit">20</field>
        </record>

        <record id="action_res_config_settings" model="ir.actions.act_window">
            <field name="name">General Settings</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">res.config.settings</field>
            <field name="view_mode">form</field>
            <field name="target">inline</field>
            <field name="context">{'module': 'sd_protocollo'}</field>
        </record>

        <record id="action_sd_protocollo_registro_list" model="ir.actions.act_window">
            <field name="name">Registro</field>
            <field name="res_model">sd.protocollo.registro</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <field name="limit">20</field>
        </record>

        <record id="action_sd_protocollo_registro_giornaliero_configurazione_list" model="ir.actions.act_window">
            <field name="name">Registro Giornaliero</field>
            <field name="res_model">sd.protocollo.registro.giornaliero.configurazione</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <field name="limit">20</field>
        </record>

        <record id="action_sd_protocollo_archivio_list" model="ir.actions.act_window">
            <field name="name">Archivio</field>
