<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <record id="view_sd_protocollo_mezzo_trasmissione_tree" model="ir.ui.view">
            <field name="name">view.sd.protocollo.mezzo.trasmissione.tree</field>
            <field name="model">sd.protocollo.mezzo.trasmissione</field>
            <field name="inherit_id" ref="sd_protocollo.view_sd_protocollo_mezzo_trasmissione_tree"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='nome']" position="after">
                    <field name="integrazione"/>
                </xpath>

            </field>
        </record>

        <record id="view_sd_protocollo_mezzo_trasmissione_form" model="ir.ui.view">
            <field name="name">view.sd.protocollo.mezzo.trasmissione.form</field>
            <field name="model">sd.protocollo.mezzo.trasmissione</field>
            <field name="inherit_id" ref="sd_protocollo.view_sd_protocollo_mezzo_trasmissione_form"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='nome']" position="after">
                    <field name="integrazione"/>
                </xpath>

            </field>
        </record>

    </data>
</odoo>