<odoo>
    <data>

        <record id="view_sd_dms_contact_email_address_tree" model="ir.ui.view">
            <field name="name">view.sd.dms.contact.email.address.list</field>
            <field name="model">sd.dms.contact.email.address</field>
            <field name="arch" type="xml">
                <list>
                    <field name="type"/>
                    <field name="email_address"/>
                </list>
            </field>
        </record>

        <record id="view_sd_dms_contact_email_address_form" model="ir.ui.view">
            <field name="name">view.sd.dms.contact.email.address.form</field>
            <field name="model">sd.dms.contact.email.address</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="type"/>
                            <field name="email_address"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

    </data>
</odoo>