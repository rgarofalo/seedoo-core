<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <data noupdate="0">

        <record id="view_inherit_res_config_settings_form" model="ir.ui.view">
            <field name="name">view.inherit.res.config.settings.form</field>
            <field name="model">res.config.settings</field>
            <field name="priority" eval="20"/>
            <field name="inherit_id" ref="sd_dms.view_inherit_res_config_settings_form"/>
            <field name="arch" type="xml">
                <div id="msg_module_sd_dms_titolario" position="replace"/>
                <xpath expr="//div[@name='titolario']" position="after">
                    <div class="mt8">
                        <label for="ztree_widget_config" string="Voci di titolario selezionabili"
                               style="font-weight:bold;" />
                        <field name="ztree_widget_config" class="oe_inline" required="1"/>
                    </div>
                </xpath>
            </field>
        </record>

    </data>
</odoo>
