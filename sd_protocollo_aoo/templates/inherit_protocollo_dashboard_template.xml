<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="template_protocollo_dashboard" name="protocollo_dashboard_user_info_aoo"
              inherit_id="sd_protocollo.template_protocollo_dashboard">

        <xpath expr="//t[@t-call='sd_protocollo.template_dashboard_user_info']" position="inside">
            <t t-set="mail_da_protocollare_pec" t-value="mail_da_protocollare_pec"/>
        </xpath>

    </template>


    <template id="template_dashboard_user_info" inherit_id="sd_protocollo.template_dashboard_user_info">

        <xpath expr="//div[@class='sd-info-utente']" position="after">
            <table class="table table-striped table-hover sd">
                <thead class="thead-sd">
                    <tr>
                        <th>AOO</th>
                        <th>UFFICI</th>
                    </tr>
                </thead>

                <tbody>
                    <t t-foreach="user_set_list" t-as="user_set">
                        <tr>
                            <td scope="row">
                                <t t-esc="user_set[0]"/>
                            </td>
                            <td>
                                <t t-esc="user_set[1]"/>
                            </td>
                        </tr>
                    </t>
                </tbody>
            </table>
        </xpath>
    </template>

</odoo>