<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <record id="view_sd_protocollo_registro_emergenza_form" model="ir.ui.view">
            <field name="name">view.sd.protocollo.registro.emergenza.form</field>
            <field name="model">sd.protocollo.registro.emergenza</field>
            <field name="inherit_id" ref="sd_protocollo.view_sd_protocollo_registro_emergenza_form"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='registro_id']" position="after">
                    <field name="aoo_id"
                           options="{'no_create': True, 'no_edit': True}"
                           readonly="[('field_readonly','=', False)]"
                    />
                </xpath>

            </field>
        </record>

        <record id="view_sd_protocollo_registro_emergenza_search" model="ir.ui.view">
            <field name="name">view.sd.protocollo.registro.emergenza.search</field>
            <field name="model">sd.protocollo.registro.emergenza</field>
            <field name="inherit_id" ref="sd_protocollo.view_sd_protocollo_registro_emergenza_search"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='causa']" position="after">
                    <field name="aoo_id"/>
                </xpath>

            </field>
        </record>

    </data>
</odoo>